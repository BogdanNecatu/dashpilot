{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "jest",
      "@testing-library/jest-dom",
      "node"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "global.d.ts",
    "next-env.d.ts",
    "setupTests.ts",
    "src",
    ".next/types/**/*.ts"
, "jest.config.ts"  ],
  "exclude": ["node_modules"],
}
